{
  "name": "nvii",
  "version": "1.0.3",
  "description": "Secure environment variable management CLI for modern development teams",
  "keywords": [
    "environment",
    "variables",
    "env",
    "cli",
    "security",
    "encryption",
    "development"
  ],
  "homepage": "https://nvii.bossadizenith.me",
  "repository": {
    "type": "git",
    "url": "https://github.com/bossadizenith/nvii.git",
    "directory": "packages/cli"
  },
  "main": "./dist/index.js",
  "bin": {
    "nvii": "./dist/index.js"
  },
  "types": "./dist/index.d.ts",
  "module": "./dist/index.mjs",
  "private": false,
  "scripts": {
    "build": "tsup",
    "dev": "tsc && pnpm chmod && tsc -w",
    "chmod": "chmod +x dist/index.js",
    "test:dev": "./dist/index.js --help",
    "start:dev": "ts-node src/index.ts || true",
    "start": "node dist/index.js || true"
  },
  "files": [
    "README.md",
    "LICENSE",
    "./dist"
  ],
  "license": "ISC",
  "devDependencies": {
    "@nvii/typescript-config": "workspace:*",
    "@swc/core": "^1.3.99",
    "@nvii/db": "workspace:*",
    "@nvii/env-helpers": "workspace:*",
    "@types/node": "^20.17.10",
    "commander": "^11.1.0",
    "concurrently": "^9.2.0",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsup": "^7.3.0",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "async-listen": "^3.0.1",
    "axios": "^1.7.9",
    "dotenv": "^16.4.7",
    "inquirer": "^12.4.3",
    "nanoid": "^3.3.7",
    "ora": "^7.0.1",
    "picocolors": "^1.0.0"
  }
