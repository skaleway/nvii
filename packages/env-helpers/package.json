{
  "name": "@nvii/env-helpers",
  "version": "0.0.1",
  "description": "Nvii helper functions",
  "private": true,
  "main": "./dist/index.js",
  "bin": {
    "envi": "./dist/index.js"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "types": "./dist/index.d.ts",
  "module": "./dist/index.mjs",
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts -d dist",
    "typecheck": "tsc",
    "test": "vitest --run --coverage"
  },
  "files": [
    "README.md",
    "LICENSE",
    "./dist"
  ],
  "license": "ISC",
  "devDependencies": {
    "@nvii/typescript-config": "workspace:*",
    "@nvii/db": "workspace:*",
    "@swc/core": "^1.3.99",
    "@types/node": "^20.17.10",
    "commander": "^11.1.0",
    "concurrently": "^9.2.0",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "tsup": "^7.3.0",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "async-listen": "^3.0.1",
    "axios": "^1.7.9",
    "dotenv": "^16.4.7",
    "inquirer": "^12.4.3",
    "nanoid": "^3.3.7",
    "ora": "^7.0.1",
    "picocolors": "^1.0.0"
  }
}
