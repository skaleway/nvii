{
  "extends": "@nvii/typescript-config/nextjs",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"],
      "@nvii/ui/*": ["../../packages/ui/src/*"],
      "Envi-data": ["./.nvii/"]
    },
    "plugins": [
      {
        "name": "next"
      }
    ],
    "skipLibCheck": true
  },
  "include": [
    "next-env.d.ts",
    "next.config.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "Envi.config.js"
  ],
  "exclude": ["node_modules"]
}
